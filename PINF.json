{
    "@extends": {
        "ScriptedCloud": "{{__DIRNAME__}}/../03-pinf.tools.ScriptedCloud/PINF.proto.json",
        "vm.pinf.io": "{{__DIRNAME__}}/PINF.proto.json",
        "to.pinf.dnsimple": {
            "location": "{{__DIRNAME__}}/../03-pinf.tools.ScriptedCloud/PINF.proto/to.pinf.dnsimple.proto.patch.json"
        },
        "io.pinf.db.postgresql": {
            "location": "{{__DIRNAME__}}/PINF.proto/io.pinf.db.postgresql~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "install": true
            }
        },
        "io.pinf.server.grunt": {
            "location": "{{__DIRNAME__}}/PINF.proto/io.pinf.server.grunt~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "install": true
            }
        },
        "io.pinf.server.webpack": {
            "location": "{{__DIRNAME__}}/PINF.proto/io.pinf.server.webpack~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "install": true
            }
        }
    },
    "config": {
        "ScriptedCloud.PINF.Tools/0": {
            "topLevelDomain": "pinf.io",
            "hostname": "{{env.PGS_WORKSPACE_UID}}.vm.pinf.io",
            "dirname": "vm.pinf.io~{{env.PGS_WORKSPACE_UID}}",
            "privateKeyPath": "{{env.HOME}}/.ssh/vm.pinf.io-{{env.PGS_WORKSPACE_UID}}.rsa"
        },
        "ScriptedCloud.PINF.Tools/boot/0": {
            "$depends": [
                "dns",
                "pio-sync-services",
                "services-proxy"
            ],
            "$depends_DISABLED": [
                "pio-sync-devtools"
            ]
        }
    },
    "mappings": {
        "io.pinf.proxy": {
            "install": true
        },
        "io.pinf.pio.postsync": {
            "install": true
        },
        "vfs-local": {
            "install": true
        },
        "vfs-socket": {
            "install": true
        },
        "io.pinf.pio.sync": {
            "install": true
        },
        "pinf.io-for-openshift": {
            "install": true
        },
        "pinf.io-for-ubuntu": {
            "install": true
        },
        "pinf.io-for-unix": {
            "install": true
        },
        "pinf-to-process": {
            "install": true
        }
    },
    "@overlays": [
        "{{env.PIO_PROFILE_PATH}}"
    ]
}