{
    "config": {
        "github.com/pinf-to/pinf-to-process/0": {
            "$to": {
                "services-proxy": {
                    "basePath": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~io.pinf.proxy~0/source/installed/master",
                    "commands": {
                        "start": "node server.js"
                    },
                    "boot": {
                        "config": "io.pinf.proxy/0"
                    },
                    "config": {
                        "io.pinf.proxy/0": {
                            "port": 8081,
                            "bind": "127.0.0.1"
                        }
                    }
                }
            }
        },
        "github.com/pinf-to/pinf-to-openshift/0": {
            "openshift": {
                "cartridge": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~pinf.io-for-openshift~0/source/installed/master"
            }
        },
        "github.com/pinf-io/io.pinf.pio.sync/0": {
            "$to": {
                "pio-sync-services": {
                    "ssh": {
                        "ip": "{{$from.server-services.vm.ip}}",
                        "user": "{{$from.server-services.dropplet.sshUser}}",
                        "privateKeyPath": "{{$from.server-services.dropplet.keyPath}}"
                    },
                    "vfs-socket-server": {
                        "port": 8086,
                        "bind": "{{$from.server-services.vm.ip}}",
                        "privateKeyPath": "{{$from.server-services.dropplet.keyPath}}"
                    },
                    "env": "{{$from.server-services.env}}",
                    "adapters": {
                        "os": "pinf.io-for-ubuntu"
                    },
                    "services": {
                        "bash.origin": {
                            "location": "bash.origin"
                        },
                        "pinf.io-for-ubuntu": {
                            "depends": [
                                "bash.origin"
                            ],
                            "location": "pinf.io-for-ubuntu",
                            "config": {
                                "pio.pinf.io/0": {
                                    "on": {
                                        "postactivate": "bin/install"
                                    }
                                }
                            }
                        },
                        "pinf-it-package-insight": {
                            "depends": [
                                "pinf.io-for-ubuntu"
                            ],
                            "location": "pinf-it-package-insight"
                        },
                        "smi-for-npm": {
                            "depends": [
                                "pinf.io-for-ubuntu",
                                "bash.origin",
                                "pinf-it-package-insight"
                            ],
                            "location": "smi-for-npm",
                            "config": {
                                "pio.pinf.io/0": {
                                    "on": {
                                        "postactivate": [
                                            "bin/link-to $PIO_BIN_DIRPATH/smi-for-npm",
                                            "smi-for-npm relink"
                                        ]
                                    }
                                }
                            }
                        },
                        "org.pinf.lib": {
                            "depends": [
                                "smi-for-npm"
                            ],
                            "location": "org.pinf.lib"
                        },
                        "org.pinf.genesis.lib": {
                            "depends": [
                                "org.pinf.lib"
                            ],
                            "location": "org.pinf.genesis.lib"
                        },
                        "pinf.io-for-unix": {
                            "depends": [
                                "bash.origin",
                                "org.pinf.genesis.lib"
                            ],
                            "location": "pinf.io-for-unix",
                            "config": {
                                "org.pinf.genesis.lib/0": {
                                    "api": {
                                        "consumes": {
                                            "FLAVOR": "pinf.io-for-ubuntu/0/manifested-os"
                                        }
                                    }
                                }
                            }
                        },
                        "io.pinf.pio.postsync": {
                            "depends": [
                                "bash.origin",
                                "pinf.io-for-unix",
                                "org.pinf.genesis.lib"
                            ],
                            "location": "io.pinf.pio.postsync",
                            "config": {
                                "org.pinf.genesis.lib/0": {
                                    "api": {
                                        "consumes": {
                                            "OS": "pinf.io-for-unix/0/virtual-os"
                                        }
                                    }
                                }
                            }
                        },
                        "vfs-local": {
                            "depends": [
                                "io.pinf.pio.postsync"
                            ],
                            "location": "vfs-local"
                        },
                        "vfs-socket": {
                            "depends": [
                                "vfs-local"
                            ],
                            "location": "vfs-socket"
                        },
                        "io.pinf.pio.sync": {
                            "depends": [
                                "io.pinf.pio.postsync",
                                "vfs-local",
                                "vfs-socket"
                            ],
                            "location": "io.pinf.pio.sync",
                            "config": {
                                "io.pinf.pio.sync/0": {
                                    "vfs-socket-server": {
                                        "port": 8086,
                                        "bind": "{{$from.server-services.vm.ip}}",
                                        "publicKey": "{{$from.space['$space.pinf.genesis/origin/0'].public.pem}}"
                                    }
                                }
                            }
                        },
                        "io.pinf.proxy": {
                            "depends": [
                                "io.pinf.pio.postsync",
                                "io.pinf.pio.sync"
                            ],
                            "location": "io.pinf.proxy",
                            "boot": {
                                "config": "io.pinf.proxy/0"
                            },
                            "config": {
                                "io.pinf.proxy/0": {
                                    "port": 8085,
                                    "bind": "{{$from.server-services.vm.ip}}",
                                    "services": {
                                        "io.pinf.pio.sync.vfs.rest": {
                                            "implementation": "SYNC_VFS_REST"
                                        }
                                    },
                                    "vhosts": {
                                        "io-pinf-pio-sync-vfs-socket.{{$from.system.hostname}}": "{{$from.server-services.vm.ip}}:8086"
                                    }
                                },
                                "org.pinf.genesis.lib/0": {
                                    "api": {
                                        "consumes": {
                                            "SYNC_VFS_REST": "io.pinf.pio.sync/0/vfs-rest"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "mappings": {
        "io.pinf.proxy": {
            "depends": [
                "org.pinf.genesis.lib"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~io.pinf.proxy~0/source/installed/master"
        },
        "io.pinf.pio.postsync": {
            "depends": [
                "bash.origin"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~io.pinf.pio.postsync~0/source/installed/master"
        },
        "vfs-local": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~SensePlatform~vfs-local~0/source/installed/master"
        },
        "vfs-socket": {
            "depends": [
                "vfs-local"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~vfs-socket~0/source/installed/master"
        },
        "io.pinf.pio.sync": {
            "depends": [
                "io.pinf.pio.postsync",
                "io.pinf.proxy",
                "bash.origin",
                "org.sourcemint.genesis.lib",
                "vfs-socket",
                "vfs-local"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~io.pinf.pio.sync~0/source/installed/master"
        },
        "pinf.io-for-openshift": {
            "depends": [
                "io.pinf.pio.postsync",
                "io.pinf.proxy",
                "bash.origin"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~pinf.io-for-openshift~0/source/installed/master"
        },
        "pinf.io-for-ubuntu": {
            "depends": [
                "bash.origin"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~pinf.io-for-ubuntu~0/source/installed/master"
        },
        "pinf.io-for-unix": {
            "depends": [
                "bash.origin",
                "org.pinf.genesis.lib"
            ],
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-io~pinf.io-for-unix~0/source/installed/master"
        },
        "pinf-to-process": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-to~pinf-to-process~0/source/installed/master"
        }
    }
}